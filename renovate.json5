{
  $schema: "https://docs.renovatebot.com/renovate-schema.json",
  extends: ["config:recommended", "github>aquaproj/aqua-renovate-config#2.3.1"],
  labels: [
    "renovate:{{depName}}",
    "renovate:{{depName}}/{{newVersion}}",
    "datasource:{{datasource}}",
  ],
  patch: {
    automerge: true,
  },
  customManagers: [
    {
      customType: "regex",
      fileMatch: ["\\.tf$"],
      matchStrings: [
        'image_identifier\\s*=\\s*"(?<depName>[^:]+?):(?<currentValue>.+?)"',
      ],
      datasourceTemplate: "docker",
      registryUrlTemplate: "https://943566097936.dkr.ecr.ap-northeast-1.amazonaws.com",
    },
  ],
}
